X	#if( 0 == 1 )
S	{
S		if( pvReturn == NULL )
S		{
S			extern void vApplicationMallocFailedHook( void );
S			vApplicationMallocFailedHook();
S		}
S		else
S		{
S			mtCOVERAGE_TEST_MARKER();
S		}
S	}
N	#endif
