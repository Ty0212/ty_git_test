Nstatic void prvHeapInit( void )
N{
NBlockLink_t *pxFirstFreeBlock;
Nuint8_t *pucAlignedHeap;
Nsize_t uxAddress;
Nsize_t xTotalHeapSize = configTOTAL_HEAP_SIZE;
Xsize_t xTotalHeapSize = ((size_t)2048);
N
N	/* Ensure the heap starts on a correctly aligned boundary. */
N	uxAddress = ( size_t ) ucHeap;
N
N	if( ( uxAddress & portBYTE_ALIGNMENT_MASK ) != 0 )
X	if( ( uxAddress & ( 0x0007 ) ) != 0 )
N	{
N		uxAddress += ( portBYTE_ALIGNMENT - 1 );
X		uxAddress += ( 8 - 1 );
N		uxAddress &= ~( ( size_t ) portBYTE_ALIGNMENT_MASK );
X		uxAddress &= ~( ( size_t ) ( 0x0007 ) );
N		xTotalHeapSize -= uxAddress - ( size_t ) ucHeap;
N	}
N
N	pucAlignedHeap = ( uint8_t * ) uxAddress;
N
N	/* xStart is used to hold a pointer to the first item in the list of free
N	blocks.  The void cast is used to prevent compiler warnings. */
N	xStart.pxNextFreeBlock = ( void * ) pucAlignedHeap;
N	xStart.xBlockSize = ( size_t ) 0;
N
